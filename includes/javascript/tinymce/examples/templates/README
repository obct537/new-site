<a name="l00017"></a>00017 <span class="preprocessor">#include &quot;<a class="code" href="apr_8h.html" title="APR Platform Definitions.">apr.h</a>&quot;</span>        <span class="comment">/* configuration data */</span><span class="comment"></span>
<a name="l00018"></a>00018 <span class="comment">/**</span>
<a name="l00019"></a>00019 <span class="comment"> * @file apr_want.h</span>
<a name="l00020"></a>00020 <span class="comment"> * @brief APR Standard Headers Support</span>
<a name="l00021"></a>00021 <span class="comment"> *</span>
<a name="l00022"></a>00022 <span class="comment"> * &lt;PRE&gt;</span>
<a name="l00023"></a>00023 <span class="comment"> * Features:</span>
<a name="l00024"></a>00024 <span class="comment"> *</span>
<a name="l00025"></a>00025 <span class="comment"> *   APR_WANT_STRFUNC:  strcmp, strcat, strcpy, etc</span>
<a name="l00026"></a>00026 <span class="comment"> *   APR_WANT_MEMFUNC:  memcmp, memcpy, etc</span>
<a name="l00027"></a>00027 <span class="comment"> *   APR_WANT_STDIO:    &lt;stdio.h&gt; and related bits</span>
<a name="l00028"></a>00028 <span class="comment"> *   APR_WANT_IOVEC:    struct iovec</span>
<a name="l00029"></a>00029 <span class="comment"> *   APR_WANT_BYTEFUNC: htons, htonl, ntohl, ntohs</span>
<a name="l00030"></a>00030 <span class="comment"> *</span>
<a name="l00031"></a>00031 <span class="comment"> * Typical usage:</span>
<a name="l00032"></a>00032 <span class="comment"> *</span>
<a name="l00033"></a>00033 <span class="comment"> *   #define APR_WANT_STRFUNC</span>
<a name="l00034"></a>00034 <span class="comment"> *   #define APR_WANT_MEMFUNC</span>
<a name="l00035"></a>00035 <span class="comment"> *   #include &quot;apr_want.h&quot;</span>
<a name="l00036"></a>00036 <span class="comment"> *</span>
<a name="l00037"></a>00037 <span class="comment"> * The appropriate headers will be included.</span>
<a name="l00038"></a>00038 <span class="comment"> *</span>
<a name="l00039"></a>00039 <span class="comment"> * Note: it is safe to use this in a header (it won&#39;t interfere with other</span>
<a name="l00040"></a>00040 <span class="comment"> *       headers&#39; or source files&#39; use of apr_want.h)</span>
<a name="l00041"></a>00041 <span class="comment"> * &lt;/PRE&gt;</span>
<a name="l00042"></a>00042 <span class="comment"> */</span>
<a name="l00043"></a>00043 
<a name="l00044"></a>00044 <span class="comment">/* --------------------------------------------------------------------- */</span>
<a name="l00045"></a>00045 
<a name="l00046"></a>00046 <span class="preprocessor">#ifdef APR_WANT_STRFUNC</span>
<a name="l00047"></a>00047 <span class="preprocessor"></span>
<a name="l00048"></a>00048 <span class="preprocessor">#if APR_HAVE_STRING_H</span>
<a name="l00049"></a>00049 <span class="preprocessor"></span><span class="preprocessor">#include &lt;string.h&gt;</span>
<a name="l00050"></a>00050 <span class="preprocessor">#endif</span>
<a name="l00051"></a>00051 <span class="preprocessor"></span><span class="preprocessor">#if APR_HAVE_STRINGS_H</span>
<a name="l00052"></a>00052 <span class="preprocessor"></span><span class="preprocessor">#include &lt;strings.h&gt;</span>
<a name="l00053"></a>00053 <span class="preprocessor">#endif</span>
<a name="l00054"></a>00054 <span class="preprocessor"></span>
<a name="l00055"></a>00055 <span class="preprocessor">#undef APR_WANT_STRFUNC</span>
<a name="l00056"></a>00056 <span class="preprocessor"></span><span class="preprocessor">#endif</span>
<a name="l00057"></a>00057 <span class="preprocessor"></span>
<a name="l00058"></a>00058 <span class="comment">/* --------------------------------------------------------------------- */</span>
<a name="l00059"></a>00059 
<a name="l00060"></a>00060 <span class="preprocessor">#ifdef APR_WANT_MEMFUNC</span>
<a name="l00061"></a>00061 <span class="preprocessor"></span>
<a name="l00062"></a>00062 <span class="preprocessor">#if APR_HAVE_STRING_H</span>
<a name="l00063"></a>00063 <span class="preprocessor"></span><span class="preprocessor">#include &lt;string.h&gt;</span>
<a name="l00064"></a>00064 <span class="preprocessor">#endif</span>
<a name="l00065"></a>00065 <span class="preprocessor"></span>
<a name="l00066"></a>00066 <span class="preprocessor">#undef APR_WANT_MEMFUNC</span>
<a name="l00067"></a>00067 <span class="preprocessor"></span><span class="preprocessor">#endif</span>
<a name="l00068"></a>00068 <span class="preprocessor"></span>
<a name="l00069"></a>00069 <span class="comment">/* --------------------------------------------------------------------- */</span>
<a name="l00070"></a>00070 
<a name="l00071"></a>00071 <span class="preprocessor">#ifdef APR_WANT_STDIO</span>
<a name="l00072"></a>00072 <span class="preprocessor"></span>
<a name="l00073"></a>00073 <span class="preprocessor">#if APR_HAVE_STDIO_H</span>
<a name="l00074"></a>00074 <span class="preprocessor"></span><span class="preprocessor">#include &lt;stdio.h&gt;</span>
<a name="l00075"></a>00075 <span class="preprocessor">#endif</span>
<a name="l00076"></a>00076 <span class="preprocessor"></span>
<a name="l00077"></a>00077 <span class="preprocessor">#undef APR_WANT_STDIO</span>
<a name="l00078"></a>00078 <span class="preprocessor"></span><span class="preprocessor">#endif</span>
<a name="l00079"></a>00079 <span class="preprocessor"></span>
<a name="l00080"></a>00080 <span class="comment">/* --------------------------------------------------------------------- */</span>
<a name="l00081"></a>00081 
<a name="l00082"></a>00082 <span class="preprocessor">#ifdef APR_WANT_IOVEC</span>
<a name="l00083"></a>00083 <span class="preprocessor"></span>
<a name="l00084"></a>00084 <span class="preprocessor">#if APR_HAVE_IOVEC</span>
<a name="l00085"></a>00085 <span class="preprocessor"></span>
<a name="l00086"></a>00086 <span class="preprocessor">#if APR_HAVE_SYS_UIO_H</span>
<a name="l00087"></a>00087 <span class="preprocessor"></span><span class="preprocessor">#include &lt;sys/uio.h&gt;</span>
<a name="l00088"></a>00088 <span class="preprocessor">#endif</span>
<a name="l00089"></a>00089 <span class="preprocessor"></span>
<a name="l00090"></a>00090 <span class="preprocessor">#else</span>
<a name="l00091"></a>00091 <span class="preprocessor"></span>
<a name="l00092"></a>00092 <span class="keyword">struct </span>iovec
<a name="l00093"></a>00093 {
<a name="l00094"></a>00094     <span class="keywordtype">char</span> *iov_base;
<a name="l00095"></a>00095     <span class="keywordtype">size_t</span> iov_len;
<a name="l00096"></a>00096 };
<a name="l00097"></a>00097 
<a name="l00098"></a>00098 <span class="preprocessor">#endif</span>
<a name="l00099"></a>00099 <span class="preprocessor"></span>
<a name="l00100"></a>00100 <span class="comment">/* apr_want is included at several layers; redefining APR_HAVE_IOVEC</span>
<a name="l00101"></a>00101 <span class="comment"> * now to ensure that our struct is not introduced several times.</span>
<a name="l00102"></a>00102 <span class="comment"> */</span>
<a name="l00103"></a>00103 <span class="preprocessor">#undef APR_HAVE_IOVEC</span>
<a name="l00104"></a>00104 <span class="preprocessor"></span><span class="preprocessor">#define APR_HAVE_IOVEC 1</span>
<a name="l00105"></a>00105 <span class="preprocessor"></span>
<a name="l00106"></a>00106 <span class="preprocessor">#undef APR_WANT_IOVEC</span>
<a name="l00107"></a>00107 <span class="preprocessor"></span><span class="preprocessor">#endif</span>
<a name="l00108"></a>00108 <span class="preprocessor"></span>
<a name="l00109"></a>00109 <span class="comment">/* --------------------------------------------------------------------- */</span>
<a name="l00110"></a>00110 
<a name="l00111"></a>00111 <span class="preprocessor">#ifdef APR_WANT_BYTEFUNC</span>
<a name="l00112"></a>00112 <span class="preprocessor"></span>
<a name="l00113"></a>00113 <span class="comment">/* Single Unix says they are in arpa/inet.h.  Linux has them in</span>
<a name="l00114"></a>00114 <span class="comment"> * netinet/in.h.  FreeBSD has them in arpa/inet.h but requires that</span>
<a name="l00115"></a>00115 <span class="comment"> * netinet/in.h be included first.</span>
<a name="l00116"></a>00116 <span class="comment"> */</span>
<a name="l00117"></a>00117 <span class="preprocessor">#if APR_HAVE_NETINET_IN_H</span>
<a name="l00118"></a>00118 <span class="preprocessor"></span><span class="preprocessor">#include &lt;netinet/in.h&gt;</span>
<a name="l00119"></a>00119 <span class="preprocessor">#endif</span>
<a name="l00120"></a>00120 <span class="preprocessor"></span><span class="preprocessor">#if APR_HAVE_ARPA_INET_H</span>
<a name="l00121"></a>00121 <span class="preprocessor"></span><span class="preprocessor">#include &lt;arpa/inet.h&gt;</span>
<a name="l00122"></a>00122 <span class="preprocessor">#endif</span>
<a name="l00123"></a>00123 <span class="preprocessor"></span>
<a name="l00124"></a>00124 <span class="preprocessor">#undef APR_WANT_BYTEFUNC</span>
<a name="l00125"></a>00125 <span class="preprocessor"></span><span class="preprocessor">#endif</span>
<a name="l00126"></a>00126 <span class="preprocessor"></span>
<a name="l00127"></a>00127 <span class="comment">/* --------------------------------------------------------------------- */</span>
</pre></div></div>
